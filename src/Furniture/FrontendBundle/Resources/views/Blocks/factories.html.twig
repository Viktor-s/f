<div class="swiper-container" data-autoplay="0" data-loop="0" data-speed="500" data-center="0" data-slides-per-view="responsive" data-xs-slides="2" data-int-slides="2" data-sm-slides="3" data-md-slides="4" data-lg-slides="5" data-add-slides="5">
    <div class="swiper-wrapper">
        {% for factory in factories %}
            <div class="swiper-slide">
                <div class="paddings-container">
                    <div class="factory-slide-entry">
                        <a class="factory-image hover-class-1" href="{{ path('retailer_profile_partners_general', {factory: factory.id}) }}">
                            <div style="">
                            {% if factory.logoImage %}
                                <img src="{{ factory.logoImage.path|imagine_filter('s201x203') }}" alt="{{ factory.name }}" title="{{ factory.name }}" />
                            {% else %}
                                <img src="{{ '/img/201x203.png' }}" alt="{{ factory.name }}" title="{{ factory.name }}" />
                            {% endif %}
                            </div>
                        </a>

                        {#<a class="subtitle subtitle--factory" href="{{ path('factory_side_general', {factory: factory.id}) }}">{{ factory.name ? factory.name : 'Unknown' }}</a>#}
                        {# <div class="date">{{ 'frontend.factory.posted'|trans({':created': factory.createdAt.format('d M Y')}) }}</div> #}
                    </div>
                </div>
            </div>
        {% endfor %}
    </div>

    <div class="pagination"></div>
</div>